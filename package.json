{
  "name": "antd-multi-cascader",
  "version": "0.0.5",
  "description": "A multiple cascader component for antd",
  "main": "dist/index.js",
  "repository": "https://github.com/HelKyle/antd-multi-cascader",
  "author": "helkylefighting@gmail.com",
  "license": "MIT",
  "private": false,
  "keywords": [
    "antd",
    "multiple-cascader",
    "cascader",
    "component"
  ],
  "files": [
    "dist",
    "!*.test.*",
    "readme.md"
  ],
  "scripts": {
    "dev": "yarn link && yarn lessc && yarn tsc --watch",
    "compile": "yarn tsc && yarn lessc",
    "tsc": "tsc",
