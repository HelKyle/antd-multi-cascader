    "test": "jest",
    "prepublishOnly": "yarn test && rm -rf dist/ && npm run compile"
  },
  "peerDependencies": {
    "@ant-design/icons": "*",
    "antd": "*",
    "classnames": "*",
    "rc-trigger": "*",
    "react": "*"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "antd": "^4.2.0",
    "jest": "^26.4.4",
    "less": "^3.2.22",
    "rc-trigger": "^5.2.0",
    "react": "^16.8.6",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "unstated-next": "^1.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
